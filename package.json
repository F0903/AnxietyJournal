{
	"name": "anxietyjournal",
	"version": "0.12.1",
	"author": "Frederik P.",
	"description": "A simple tool for taking note of your anxiety.",
	"main": "src/main.js",
	"repository": {
		"url": "https://github.com/F0903/AnxietyJournal/",
		"type": "git"
	},
	"private": true,
	"license": "SEE LICENSE IN LICENSE",
	"scripts": {
		"build:dev": "webpack",
		"build:pub": "webpack --mode=production",
		"start": "npm run build:dev && electron out/src/main.js",
		"package": "npm run build:pub && electron-builder",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"build": {
		"appId": "AnxietyJournal",
		"productName": "Anxiety Journal",
		"files": [
			{
				"from": "./out",
				"to": "./"
			}
		],
		"directories": {
			"buildResources": "./build-resources/",
			"output": "./bin/"
		},
		"publish": {
			"provider": "github"
		},
		"win": {
			"target": [
				"nsis"
			]
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15",
		"electron-updater": "^4.3",
		"fs-extra": "^8.1",
		"i": "^0.3",
		"mongodb": "^3.6",
		"mongodb-client-encryption": "^1.2",
		"xlsx": "^0.17"
	},
	"devDependencies": {
		"@types/app-root-path": "^1.2",
		"@types/fs-extra": "^9.0",
		"@types/mongodb": "^3.6",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"app-root-path": "^3.0",
		"copy-webpack-plugin": "^9.0",
		"css-loader": "^5.2",
		"electron": "^13",
		"electron-builder": "^22",
		"electron-builder-squirrel-windows": "^22",
		"eslint": "^7.27",
		"eslint-config-prettier": "^8.3",
		"html-minimizer-webpack-plugin": "^3.1",
		"mini-css-extract-plugin": "^1.6",
		"prettier": "^2.3",
		"style-loader": "^2.0",
		"ts-loader": "^9.2",
		"typescript": "^4.3",
		"webpack": "^5.38",
		"webpack-cli": "^4.7"
	}
}
