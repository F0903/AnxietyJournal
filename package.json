{
	"name": "anxietyjournal",
	"version": "0.13.0",
	"author": "Frederik P.",
	"description": "A simple tool for taking note of your anxiety.",
	"main": "src/main.js",
	"type": "commonjs",
	"repository": {
		"url": "https://github.com/F0903/AnxietyJournal/",
		"type": "git"
	},
	"private": true,
	"license": "SEE LICENSE IN LICENSE",
	"scripts": {
		"build:dev": "webpack",
		"build:pub": "webpack --mode=production",
		"start": "npm run build:dev && electron out/src/main.js",
		"package": "npm run build:pub && electron-builder"
	},
	"build": {
		"appId": "AnxietyJournal",
		"productName": "Anxiety Journal",
		"files": [
			{
				"from": "./out",
				"to": "./"
			}
		],
		"directories": {
			"buildResources": "./build-resources/",
			"output": "./bin/"
		},
		"publish": {
			"provider": "github"
		},
		"win": {
			"target": [
				"nsis"
			]
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15",
		"electron-updater": "^4.6",
		"fs-extra": "^10",
		"i": "^0.3",
		"mongodb": "^4.3",
		"mongodb-client-encryption": "^1.2",
		"xlsx": "^0.17"
	},
	"devDependencies": {
		"@types/app-root-path": "^1.2",
		"@types/fs-extra": "^9.0",
		"@typescript-eslint/eslint-plugin": "^5",
		"@typescript-eslint/parser": "^5",
		"app-root-path": "^3.0",
		"copy-webpack-plugin": "^10.2",
		"css-loader": "^6.5",
		"electron": "^16",
		"electron-builder": "^22",
		"eslint": "^8.8",
		"eslint-config-prettier": "^8.3",
		"html-minimizer-webpack-plugin": "^3.5",
		"mini-css-extract-plugin": "^2.5",
		"prettier": "^2",
		"style-loader": "^3.3",
		"ts-loader": "^9.2",
		"typescript": "^4.5",
		"webpack": "^5.68",
		"webpack-cli": "^4.9"
	}
}
